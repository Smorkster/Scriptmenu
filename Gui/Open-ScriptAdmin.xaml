                                                Value="{Binding ElementName=GridErrorInfo, Path=DataContext.ComputerName}" />
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Border>
                        <Label x:Name="LblErrLogUserInput">
                            <Label.Style>
                                <Style TargetType="Label"
                                       BasedOn="{StaticResource TitleLabel}">
                                    <Setter Property="Grid.Row"
                                            Value="3" />
                                </Style>
                            </Label.Style>
                        </Label>
                        <Border>
                            <Border.Style>
                                <Style TargetType="Border"
                                       BasedOn="{StaticResource BorderBaseStyle}">
                                    <Setter Property="Grid.Column"
                                            Value="1" />
                                    <Setter Property="Grid.ColumnSpan"
                                            Value="3" />
                                    <Setter Property="Grid.Row"
                                            Value="3" />
                                </Style>
                            </Border.Style>
                            <TextBlock x:Name="TblErrLogUserInput">
                                <TextBlock.Style>
                                    <Style TargetType="TextBlock"
                                           BasedOn="{StaticResource TblBaseStyle}">
                                        <Setter Property="Text"
                                                Value="{Binding ElementName=GridErrorInfo, Path=DataContext.UserInput}" />
                                        <Setter Property="TextAlignment"
                                                Value="Justify" />
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Border>
                        <Label x:Name="LblErrLogErrorMessage">
                            <Label.Style>
                                <Style TargetType="Label"
                                       BasedOn="{StaticResource TitleLabel}">
                                    <Setter Property="Grid.Row"
                                            Value="4" />
                                </Style>
                            </Label.Style>
                        </Label>
                        <Border>
                            <Border.Style>
                                <Style TargetType="Border"
                                       BasedOn="{StaticResource BorderBaseStyle}">
                                    <Setter Property="Grid.Column"
                                            Value="1" />
                                    <Setter Property="Grid.ColumnSpan"
                                            Value="3" />
                                    <Setter Property="Grid.Row"
                                            Value="4" />
                                </Style>
                            </Border.Style>
                            <TextBlock x:Name="TblErrLogErrorMessage">
                                <TextBlock.Style>
                                    <Style TargetType="TextBlock"
                                           BasedOn="{StaticResource TblBaseStyle}">
                                        <Setter Property="Text"
                                                Value="{Binding ElementName=GridErrorInfo, Path=DataContext.ErrorMessage}" />
                                        <Setter Property="TextAlignment"
                                                Value="Justify" />
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Border>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem x:Name="TiRollback">
                <TabItem.Style>
                    <Style TargetType="TabItem"
