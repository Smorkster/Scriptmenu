                                          MinWidth="300"
                                          Width="Auto" />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <ListView x:Name="LvRollbackFileNames">
                        <ListView.Style>
                            <Style TargetType="ListView"
                                   BasedOn="{StaticResource ListViewBaseStyle}">
                                <Setter Property="DisplayMemberPath"
                                        Value="Script" />
                                <Setter Property="Grid.RowSpan"
                                        Value="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=RowDefinitions.Count}" />
                            </Style>
                        </ListView.Style>
                    </ListView>
                    <Grid>
                        <Grid.Style>
                            <Style TargetType="Grid">
                                <Setter Property="Grid.Column"
                                        Value="1" />
                            </Style>
                        </Grid.Style>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Button x:Name="BtnListRollbacks">
                            <Button.Style>
                                <Style TargetType="Button"
                                       BasedOn="{StaticResource BtnBaseStyle}">
                                </Style>
                            </Button.Style>
                        </Button>
                        <Button x:Name="BtnOpenRollbackFile">
                            <Button.Style>
                                <Style TargetType="Button"
                                       BasedOn="{StaticResource BtnBaseStyle}">
                                    <Setter Property="Grid.Column"
                                            Value="1" />
                                </Style>
                            </Button.Style>
                        </Button>
                        <Button x:Name="BtnDoRollback">
                            <Button.Style>
                                <Style TargetType="Button"
                                       BasedOn="{StaticResource BtnBaseStyle}">
                                    <Setter Property="Grid.Column"
                                            Value="2" />
                                </Style>
                            </Button.Style>
                        </Button>
                        <Button x:Name="BtnCopyRollbackInfo">
                            <Button.Style>
                                <Style TargetType="Button"
                                       BasedOn="{StaticResource BtnBaseStyle}">
                                    <Setter Property="Grid.Row"
                                            Value="1" />
                                    <Setter Property="IsEnabled"
                                            Value="True" />
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding ElementName=LvRollbackFileNames, Path=SelectedItem}"
                                                     Value="{x:Null}">
                                            <Setter Property="IsEnabled"
                                                    Value="False" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                        </Button>
                    </Grid>
                    <Grid x:Name="GridRollbackFilterOptions">
                        <Grid.Style>
                            <Style TargetType="Grid">
                                <Setter Property="Grid.Column"
                                        Value="1" />
                                <Setter Property="Grid.Row"
                                        Value="1" />
                            </Style>
                        </Grid.Style>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Label x:Name="LblRollbackFiltersTitle">
                            <Label.Style>
                                <Style TargetType="Label"
                                       BasedOn="{StaticResource TitleLabel}">
                                    <Setter Property="FontSize"
                                            Value="14" />
                                    <Setter Property="FontWeight"
                                            Value="Bold" />
                                    <Setter Property="Grid.ColumnSpan"
                                            Value="2" />
                                    <Setter Property="VerticalAlignment"
                                            Value="Center" />
                                </Style>
                            </Label.Style>
                        </Label>
                        <Label x:Name="LblRollbackFilterTypeTitle">
                            <Label.Style>
                                <Style TargetType="Label"
                                       BasedOn="{StaticResource TitleLabel}">
                                    <Setter Property="Grid.Row"
                                            Value="1" />
                                </Style>
                            </Label.Style>
                        </Label>
                        <StackPanel>
                            <StackPanel.Style>
                                <Style TargetType="StackPanel"
                                       BasedOn="{StaticResource SpBaseStyle}">
                                    <Setter Property="Grid.Column"
                                            Value="1" />
                                    <Setter Property="Grid.Row"
                                            Value="1" />
                                </Style>
                            </StackPanel.Style>
                            <CheckBox x:Name="CbRollbackFilterTypePs1">
                                <CheckBox.Style>
                                    <Style TargetType="CheckBox"
                                           BasedOn="{StaticResource ChbSurveyStyle}" />
                                </CheckBox.Style>
                            </CheckBox>
                            <CheckBox x:Name="CbRollbackFilterTypePsd1">
                                <CheckBox.Style>
                                    <Style TargetType="CheckBox"
                                           BasedOn="{StaticResource ChbSurveyStyle}" />
                                </CheckBox.Style>
                            </CheckBox>
                            <CheckBox x:Name="CbRollbackFilterTypePsm1">
                                <CheckBox.Style>
                                    <Style TargetType="CheckBox"
                                           BasedOn="{StaticResource ChbSurveyStyle}" />
                                </CheckBox.Style>
                            </CheckBox>
                            <CheckBox x:Name="CbRollbackFilterTypeTxt">
                                <CheckBox.Style>
                                    <Style TargetType="CheckBox"
                                           BasedOn="{StaticResource ChbSurveyStyle}" />
                                </CheckBox.Style>
                            </CheckBox>
                            <CheckBox x:Name="CbRollbackFilterTypeXaml">
                                <CheckBox.Style>
                                    <Style TargetType="CheckBox"
                                           BasedOn="{StaticResource ChbSurveyStyle}" />
                                </CheckBox.Style>
                            </CheckBox>
                        </StackPanel>
                    </Grid>
                    <ProgressBar x:Name="PbListingRollbacks">
                        <ProgressBar.Style>
                            <Style TargetType="ProgressBar">
                                <Setter Property="Grid.Column"
                                        Value="1" />
                                <Setter Property="Grid.Row"
                                        Value="1" />
                                <Setter Property="Height"
                                        Value="{Binding ElementName=SpRollbackFilterOptions, Path=Height}" />
                                <Setter Property="IsIndeterminate"
                                        Value="True" />
                                <Setter Property="Margin"
                                        Value="0" />
                                <Setter Property="Visibility"
                                        Value="Collapsed" />
                            </Style>
                        </ProgressBar.Style>
                    </ProgressBar>
                    <DataGrid x:Name="DgRollbacks">
                        <DataGrid.Style>
                            <Style TargetType="DataGrid"
                                   BasedOn="{StaticResource DgBaseStyle}">
                                <Setter Property="Grid.Column"
                                        Value="1" />
                                <Setter Property="Grid.Row"
                                        Value="2" />
                                <Setter Property="Grid.RowSpan"
                                        Value="2" />
                                <Setter Property="ItemsSource"
                                        Value="{Binding ElementName=LvRollbackFileNames, Path=SelectedItem.FileLogs}" />
                                <Setter Property="RowStyle"
                                        Value="{StaticResource DgRollbackRowStyle}" />
                            </Style>
                        </DataGrid.Style>
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding File}"
                                                ElementStyle="{StaticResource ColumnElementStyle}">
                                <DataGridTextColumn.HeaderStyle>
                                    <Style TargetType="DataGridColumnHeader"
                                           BasedOn="{StaticResource DgchBaseStyle}" />
                                </DataGridTextColumn.HeaderStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn Binding="{Binding Updated, StringFormat={}{0:yyyy-MM-dd HH:mm:ss}}"
                                                ElementStyle="{StaticResource DateCell}">
                                <DataGridTextColumn.HeaderStyle>
                                    <Style TargetType="DataGridColumnHeader"
                                           BasedOn="{StaticResource DgchBaseStyle}" />
                                </DataGridTextColumn.HeaderStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn Binding="{Binding UpdatedBy, Converter={StaticResource Converter}}"
                                                ElementStyle="{StaticResource NameCell}">
                                <DataGridTextColumn.HeaderStyle>
                                    <Style TargetType="DataGridColumnHeader"
                                           BasedOn="{StaticResource DgchBaseStyle}" />
                                </DataGridTextColumn.HeaderStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn Binding="{Binding Type}"
                                                ElementStyle="{StaticResource ColumnElementStyleCenter}">
                                <DataGridTextColumn.HeaderStyle>
                                    <Style TargetType="DataGridColumnHeader"
                                           BasedOn="{StaticResource DgchBaseStyle}" />
                                </DataGridTextColumn.HeaderStyle>
                            </DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>
            <TabItem x:Name="TiSurveys">
                <TabItem.Style>
                    <Style TargetType="TabItem"
                           BasedOn="{StaticResource TabItemBaseStyle}" />
                </TabItem.Style>
                <Grid>
                    <Grid.Resources>
                        <CollectionViewSource x:Key="CvsSurveys"
                                              Source="{Binding ElementName=DgSurveyScripts, Path=SelectedItem.Surveys}">
                            <CollectionViewSource.SortDescriptions>
                                <componentmodel:SortDescription Direction="Descending"
                                                                PropertyName="Name" />
                            </CollectionViewSource.SortDescriptions>
                        </CollectionViewSource>
                    </Grid.Resources>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid>
                        <Button x:Name="BtnListSurveys"
                                Style="{StaticResource BtnBaseStyle}" />
                        <ProgressBar x:Name="PbParseSurveys">
                            <ProgressBar.Style>
                                <Style TargetType="ProgressBar">
                                    <Setter Property="Margin"
                                            Value="5" />
                                    <Setter Property="Visibility"
                                            Value="Collapsed" />
                                </Style>
                            </ProgressBar.Style>
                        </ProgressBar>
                    </Grid>
                    <DataGrid x:Name="DgSurveyScripts">
                        <DataGrid.Style>
                            <Style TargetType="DataGrid"
                                   BasedOn="{StaticResource DgBaseStyle}">
                                <Setter Property="Grid.Row"
                                        Value="1" />
                                <Setter Property="Grid.RowSpan"
                                        Value="3" />
                            </Style>
                        </DataGrid.Style>
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding ScriptName}"
                                                ElementStyle="{StaticResource ColumnElementStyle}"
                                                Width="220">
                                <DataGridTextColumn.HeaderStyle>
                                    <Style TargetType="DataGridColumnHeader"
                                           BasedOn="{StaticResource DgchBaseStyle}" />
                                </DataGridTextColumn.HeaderStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn Binding="{Binding SurvaceyCount}"
                                                ElementStyle="{StaticResource ColumnElementStyle}">
                                <DataGridTextColumn.HeaderStyle>
                                    <Style TargetType="DataGridColumnHeader"
                                           BasedOn="{StaticResource DgchBaseStyle}" />
                                </DataGridTextColumn.HeaderStyle>
                            </DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                    <Label x:Name="LblSurveyTotSumTitle">
                        <Label.Style>
                            <Style TargetType="Label"
                                   BasedOn="{StaticResource TitleLabel}">
                                <Setter Property="Grid.Column"
                                        Value="1" />
                            </Style>
                        </Label.Style>
                    </Label>
                    <Border>
                        <Border.Style>
                            <Style TargetType="Border"
                                   BasedOn="{StaticResource BorderBaseStyle}">
                                <Setter Property="Grid.Column"
                                        Value="2" />
                            </Style>
                        </Border.Style>
                        <TextBlock x:Name="TblSurveyTotSum">
                            <TextBlock.Style>
                                <Style TargetType="TextBlock"
                                       BasedOn="{StaticResource TblBaseStyle}">
                                    <Setter Property="Text"
                                            Value="{Binding ElementName=DgSurveyScripts, Path=SelectedItem.Surveys.Count}" />
                                </Style>
                            </TextBlock.Style>
                        </TextBlock>
                    </Border>
                    <Label x:Name="LblSurveyScriptVersionTitle">
                        <Label.Style>
                            <Style TargetType="Label"
                                   BasedOn="{StaticResource TitleLabel}">
                                <Setter Property="Grid.Column"
                                        Value="1" />
                                <Setter Property="Grid.Row"
                                        Value="1" />
                            </Style>
                        </Label.Style>
                    </Label>
                    <ComboBox x:Name="CbSurveyScriptVersion">
                        <ComboBox.Style>
                            <Style TargetType="ComboBox"
                                   BasedOn="{StaticResource CbBaseStyle}">
                                <Setter Property="DisplayMemberPath"
                                        Value="Key" />
                                <Setter Property="Grid.Column"
                                        Value="2" />
                                <Setter Property="Grid.Row"
                                        Value="1" />
                                <Setter Property="IsEnabled"
                                        Value="True" />
                                <Setter Property="ItemsSource"
                                        Value="{Binding Source={StaticResource CvsSurveys}}" />
                                <Setter Property="SelectedIndex"
                                        Value="0" />
                                <Style.Triggers>
                                    <Trigger Property="HasItems"
                                             Value="False">
                                        <Setter Property="IsEnabled"
                                                Value="False" />
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </ComboBox.Style>
                    </ComboBox>
                    <Label x:Name="LblSurveyFilterTitle">
                        <Label.Style>
                            <Style TargetType="Label"
                                   BasedOn="{StaticResource TitleLabel}">
                                <Setter Property="Grid.Column"
                                        Value="1" />
                                <Setter Property="Grid.Row"
                                        Value="2" />
                            </Style>
                        </Label.Style>
                    </Label>
                    <StackPanel>
                        <StackPanel.Style>
                            <Style TargetType="StackPanel"
                                   BasedOn="{StaticResource SpBaseStyle}">
                                <Setter Property="Grid.Column"
                                        Value="2" />
                                <Setter Property="Grid.Row"
                                        Value="2" />
                            </Style>
                        </StackPanel.Style>
                        <CheckBox x:Name="CbSurveyFilterRating1">
                            <CheckBox.Style>
                                <Style TargetType="CheckBox"
                                       BasedOn="{StaticResource ChbBaseStyle}">
                                    <Setter Property="Content"
                                            Value="1" />
                                </Style>
                            </CheckBox.Style>
                        </CheckBox>
                        <CheckBox x:Name="CbSurveyFilterRating2">
                            <CheckBox.Style>
                                <Style TargetType="CheckBox"
                                       BasedOn="{StaticResource ChbBaseStyle}">
                                    <Setter Property="Content"
                                            Value="2" />
                                </Style>
                            </CheckBox.Style>
                        </CheckBox>
                        <CheckBox x:Name="CbSurveyFilterRating3">
                            <CheckBox.Style>
                                <Style TargetType="CheckBox"
                                       BasedOn="{StaticResource ChbBaseStyle}">
                                    <Setter Property="Content"
                                            Value="3" />
                                </Style>
                            </CheckBox.Style>
                        </CheckBox>
                        <CheckBox x:Name="CbSurveyFilterRating4">
                            <CheckBox.Style>
                                <Style TargetType="CheckBox"
                                       BasedOn="{StaticResource ChbBaseStyle}">
                                    <Setter Property="Content"
                                            Value="4" />
                                </Style>
                            </CheckBox.Style>
                        </CheckBox>
                        <CheckBox x:Name="CbSurveyFilterRating5">
                            <CheckBox.Style>
                                <Style TargetType="CheckBox"
                                       BasedOn="{StaticResource ChbBaseStyle}">
                                    <Setter Property="Content"
                                            Value="5" />
                                </Style>
                            </CheckBox.Style>
                        </CheckBox>
                    </StackPanel>
                    <DataGrid x:Name="DgSurveyAnswers">
                        <DataGrid.Style>
                            <Style TargetType="DataGrid"
                                   BasedOn="{StaticResource DgBaseStyle}">
                                <Setter Property="Grid.Column"
                                        Value="1" />
                                <Setter Property="Grid.ColumnSpan"
                                        Value="3" />
                                <Setter Property="Grid.Row"
                                        Value="3" />
                                <Setter Property="ItemsSource"
                                        Value="{Binding ElementName=CbSurveyScriptVersion, Path=SelectedItem.Value}" />
                                <Setter Property="RowStyle"
                                        Value="{StaticResource DgSurveyRowStyle}" />
                            </Style>
                        </DataGrid.Style>
                        <DataGrid.Columns>
                            <DataGridTemplateColumn Width="1*">
                                <DataGridTemplateColumn.HeaderStyle>
                                    <Style TargetType="DataGridColumnHeader"
                                           BasedOn="{StaticResource DgchBaseStyle}" />
                                </DataGridTemplateColumn.HeaderStyle>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock>
                                            <TextBlock.Style>
                                                <Style TargetType="TextBlock"
                                                       BasedOn="{StaticResource TblBaseStyle}">
                                                    <Setter Property="Text"
                                                            Value="{Binding Path=Comment}" />
                                                    <Style.Triggers>
                                                        <Trigger Property="Text"
                                                                 Value="{x:Static sys:String.Empty}">
                                                            <Setter Property="Background"
                                                                    Value="LightGray" />
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </TextBlock.Style>
                                        </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTextColumn Binding="{Binding Rating}"
                                                ElementStyle="{StaticResource ColumnElementStyleCenter}"
                                                Width="70">
                                <DataGridTextColumn.HeaderStyle>
                                    <Style TargetType="DataGridColumnHeader"
                                           BasedOn="{StaticResource DgchBaseStyle}" />
                                </DataGridTextColumn.HeaderStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn Binding="{Binding Operator, Converter={StaticResource Converter}}"
                                                ElementStyle="{StaticResource ColumnElementStyle}"
                                                Width="Auto">
                                <DataGridTextColumn.HeaderStyle>
                                    <Style TargetType="DataGridColumnHeader"
                                           BasedOn="{StaticResource DgchBaseStyle}" />
                                </DataGridTextColumn.HeaderStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn Binding="{Binding LogDate}"
                                                ElementStyle="{StaticResource ColumnElementStyle}"
                                                Width="Auto">
                                <DataGridTextColumn.HeaderStyle>
                                    <Style TargetType="DataGridColumnHeader"
                                           BasedOn="{StaticResource DgchBaseStyle}" />
                                </DataGridTextColumn.HeaderStyle>
                            </DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
